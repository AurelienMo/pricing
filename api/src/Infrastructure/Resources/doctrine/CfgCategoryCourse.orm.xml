<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Pricing\Infrastructure\Doctrine\Entity\CfgCategoryCourse" table="cfg_category_course">
        <id name="id" column="id" type="string">
            <generator strategy="NONE" />
        </id>
        <field name="name" column="name" nullable="false"/>
        <field name="createdAt" column="created_at" type="datetime" nullable="false" />
        <field name="updatedAt" column="updated_at" type="datetime" nullable="false" />
        <one-to-many field="courses" target-entity="Pricing\Infrastructure\Doctrine\Entity\CfgCourse" mapped-by="categoryCourse" fetch="EAGER"/>
    </entity>
</doctrine-mapping>
